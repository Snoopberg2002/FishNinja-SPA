  \r\n        //create user in Firebase\r\n  \r\n        auth.createUserWithEmailAndPassword(email, password).then(response => {\r\n  \r\n          console.log(\"user registered\");\r\n          AddUser(response.user.uid); \r\n          \r\n        });  \r\n\r\n        function AddUser(id) {\r\n\r\n          console.log(id);\r\n          \r\n          let newUser = {\r\n          \r\n            UserName: userName,\r\n        \r\n            HomeCity: homeCity,\r\n  \r\n            HomeState: homeState,\r\n\r\n            FavoriteLake: favoriteLake,\r\n\r\n            Gender: gender,\r\n        \r\n            Email: email,\r\n\r\n            Id: id\r\n        \r\n          }\r\n          \r\n          users.add(newUser)\r\n          .then(res => {\r\n            console.log(res);\r\n            toggleModal(document.querySelector(\".loginTemplate\").innerHTML);\r\n            alert(\"Please re-enter email and password to Log In.\");\r\n          })\r\n          .catch(e => {\r\n            console.log(e);\r\n          })\r\n        }\r\n    });\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"ba90b4ad7561882816787e86ca432650","cacheData":{"env":{}}}